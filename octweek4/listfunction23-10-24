list1=input("enter a list").split()
list2=list(map(int,list1))
sort=sorted(list2)
print("sorted list",sort)
max=max(list2)
print("maximum value",max)
min=min(list2)
print("minimum value",min)
sort.reverse()
print("reverse",sort)
count=len(list2)
print("length of list",count)
sum=sum(list2)
print("sum of list",sum)
